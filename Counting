const int button = 2;
bool laststate=0;
int i=0;
void setup() 
{
pinMode(button, INPUT);
Serial.begin (9600); 
}  


void loop() {
  bool current= digitalRead(button);
  if (current != laststate)
 {  delay(100);
    
    i++;
    Serial.print("Count= ");
    Serial.println(i);
    delay(100);
    bool laststate= digitalRead(button);
   if (i==10)
   {
     i=0;
   }
 }
 }
  
